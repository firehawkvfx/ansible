{
  "Version": "2012-10-17",
  "Id": "s3ProdDevSharePolicy",
  "Statement": [
    {
      "Action": [
        "s3:*"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::{{ installers_bucket }}",
        "arn:aws:s3:::{{ installers_bucket }}/*"
      ],
      "Principal": {
        "AWS": [
          "{{ AWS_ACCOUNT_ID_dev }}", "{{ AWS_ACCOUNT_ID_prod }}"
        ]
      }
    }
  ]
}