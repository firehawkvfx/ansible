{
  "Version": "2012-10-17",
  "Id": "s3DevProdPolicy",
  "Statement": [
    {
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:DeleteObject"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::{{ installers_bucket }}/*",
      "Principal": {
        "AWS": [
          "{{ AWS_ACCOUNT_ID_dev }}", "{{ AWS_ACCOUNT_ID_prod }}"
        ]
      }
    }
  ]
}